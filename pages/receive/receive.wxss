.item .sty_1{
  position: absolute;
  top: 60%;
  left: 0;
  color:#fff;
  width:100%;
  text-align: center;
}
.item .sty_2{

    font-size:48rpx;
}
.item .sty_3{
  font-size:28rpx;
}
image{
  width: 100%;
  height: 100%;
}
 .item:nth-child(1){
  transform: translate3d(17%, 0, 0) scale(1);
  transform-origin: 0 50%;
  opacity: 1;
  z-index: 3;
}
 .item:nth-child(2){
  transform: translate3d(28%, 0, 0) scale(0.9);
  transform-origin: 100% 50%;
  opacity: 0.2;
  z-index: 2;
}
 .item:nth-child(3){
  transform: translate3d(7%, 0, 0) scale(0.9);
  transform-origin: 0 50%;
  opacity: 0.2;
  z-index: 2;
}
.swiper .item:nth-child(4){
  transform: translate3d(-34%, 0, 0) scale(0.9);
}
.btn_l{
  position: absolute;
  z-index: 101;
  margin-top: 200rpx;
  left:0;
}
.btn_r{
  position: absolute;
  z-index: 101;
  margin-top: 200rpx;
  right:0;
}
.btnz, .btny{
  width: 65rpx;
